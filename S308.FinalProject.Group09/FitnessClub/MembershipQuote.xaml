        <Rectangle HorizontalAlignment="Left" Height="124" Margin="22,38,0,0" Stroke="Black" VerticalAlignment="Top" Width="595"/>
        <Label x:Name="lblMembershipType" Content="Membership Type: " HorizontalAlignment="Left" Margin="28,52,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="11"/>
        <TextBox x:Name="txtMembershipType" HorizontalAlignment="Left" Height="23" Margin="137,54,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="105" Text="General"/>
        <Label x:Name="lblStartDate_Copy" Content="Start Date: " HorizontalAlignment="Left" Margin="252,52,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="11"/>
